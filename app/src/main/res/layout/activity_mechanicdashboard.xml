<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="mechanicdashboardVM" type="com.repairpal.app.modules.mechanicdashboard.data.viewmodel.MechanicdashboardVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnyourlocation"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackyourlocation"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtYourLocation"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_136pxh"
                android:layout_marginStart="@dimen/_136pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtYourLocation}"
                style="@style/txtInterbold12" tools:text="@string/lbl_your_location"/>
            <ImageView android:id="@+id/imageFindOne"
                android:layout_gravity="top|center"
                android:layout_height="@dimen/_482pxh"
                android:layout_marginBottom="@dimen/_69pxv"
                android:layout_marginEnd="@dimen/_11pxh"
                android:layout_marginStart="@dimen/_11pxh"
                android:layout_marginTop="@dimen/_69pxv"
                android:layout_width="@dimen/_337pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_find1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_find1"/>
            <LinearLayout android:id="@+id/linearColumnstartinspectio"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_165pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_165pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_900">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtStartinspectio"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_112pxh"
                    android:layout_marginStart="@dimen/_112pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtStartinspectio}"
                    style="@style/txtInterbold12_1" tools:text="@string/msg_start_inspectio"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearColumncancelrequest"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_66pxv"
                android:layout_marginEnd="@dimen/_98pxh"
                android:layout_marginStart="@dimen/_98pxh"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleindigo_900">
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtCancelrequest"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_31pxh"
                    android:layout_marginStart="@dimen/_31pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtCancelrequest}"
                    style="@style/txtInterbold12_1" tools:text="@string/lbl_cancel_request"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageComputer"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_32pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_26pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_computer"
                tools:ignore="ContentDescription" tools:src="@drawable/img_computer"/>
            <View android:background="@color/black_900"
                android:id="@+id/lineLineOne"
                android:layout_gravity="center"
                android:layout_height="@dimen/_800pxv"
                android:layout_marginEnd="@dimen/_40pxh"
                android:layout_marginStart="@dimen/_40pxh" android:layout_width="@dimen/_1pxh"/>
            <LinearLayout android:id="@+id/linearColumnlineoneOne"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content" android:orientation="vertical">
                <LinearLayout android:id="@+id/linearColumnperone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylegray_500">
                    <androidx.appcompat.widget.Toolbar
                        android:gravity="end"
                        android:id="@+id/toolbarToolbar"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_15pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent" app:contentInsetStart="0dp">
                        <LinearLayout android:id="@+id/linearRowperone"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView android:id="@+id/imagePerOne"
                                android:layout_height="@dimen/_108pxh"
                                android:layout_marginTop="@dimen/_14pxv"
                                android:layout_width="@dimen/_90pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_per1}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_per1"/>
                            <FrameLayout
                                android:id="@+id/frameStackclose"
                                android:layout_height="@dimen/_28pxv"
                                android:layout_marginBottom="@dimen/_94pxv"
                                android:layout_marginStart="@dimen/_12pxh" android:layout_width="@dimen/_28pxh">
                                <ImageView android:id="@+id/imageClose"
                                    android:layout_gravity="top|center"
                                    android:layout_height="@dimen/_14pxh"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_marginEnd="@dimen/_7pxh"
                                    android:layout_marginStart="@dimen/_7pxh"
                                    android:layout_marginTop="@dimen/_6pxv"
                                    android:layout_width="@dimen/_14pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_close"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                                <View
                                    android:background="@drawable/rectangle_bg_black_900_radius_14"
                                    android:id="@+id/viewVector"
                                    android:layout_gravity="start"
                                    android:layout_height="@dimen/_28pxh" android:layout_width="@dimen/_28pxh"/>
                            </FrameLayout>
                        </LinearLayout>
                    </androidx.appcompat.widget.Toolbar>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowuser"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_27pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageUser"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_user"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                        <TextView android:id="@+id/txtWelcomeDavid"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_weight="0.77"
                            android:layout_width="0dp"
                            android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtWelcomeDavid}"
                            style="@style/txtInterregular12" tools:text="@string/msg_welcome_david"/>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacklineoneOne"
                    android:layout_height="@dimen/_615pxv" android:layout_width="@dimen/_180pxh">
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLineOneOne"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_7pxh" android:layout_width="@dimen/_173pxh"/>
                    <LinearLayout android:id="@+id/linearColumnhome"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_1pxh"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_A700">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowhome"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_27pxh"
                            android:layout_marginStart="@dimen/_27pxh"
                            android:layout_marginTop="@dimen/_23pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageHome"
                                android:layout_height="@dimen/_17pxh"
                                android:layout_width="@dimen/_20pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_home"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHomePage"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_14pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.77"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtHomePage}"
                                style="@style/txtInterregular12" tools:text="@string/lbl_home_page"/>
                        </LinearLayout>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLineTwo"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_179pxh"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowclock"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_29pxh"
                            android:layout_marginStart="@dimen/_29pxh"
                            android:layout_marginTop="@dimen/_27pxv"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView android:id="@+id/imageClock"
                                android:layout_height="@dimen/_18pxh"
                                android:layout_width="@dimen/_18pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_clock"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtHistory"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_13pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.69"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtHistory}"
                                style="@style/txtInterregular12" tools:text="@string/lbl_history"/>
                        </LinearLayout>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLineTwoOne"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_26pxv" android:layout_width="@dimen/_178pxh"/>
                        <LinearLayout
                            android:id="@+id/linearColumnvectorOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_342pxv"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_20pxv"
                            android:layout_width="match_parent" android:orientation="vertical">
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowvectorOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_25pxh"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <FrameLayout
                                    android:id="@+id/frameStackvectorOne"
                                    android:layout_height="@dimen/_20pxv" android:layout_width="@dimen/_19pxh">
                                    <ImageView
                                    android:id="@+id/imageVectorOne"
                                    android:layout_gravity="bottom|end"
                                    android:layout_height="@dimen/_1pxh"
                                    android:layout_marginEnd="@dimen/_7pxh"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="@dimen/_3pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_vector"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                                    <ImageView
                                    android:id="@+id/imageNotification"
                                    android:layout_gravity="top|start"
                                    android:layout_height="@dimen/_18pxv"
                                    android:layout_marginBottom="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_notification_black_900"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_notification_black_900"/>
                                </FrameLayout>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtNotificationOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_weight="0.77"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtNotificationOne}"
                                    style="@style/txtInterregular12" tools:text="@string/lbl_notification"/>
                            </LinearLayout>
                            <LinearLayout
                                android:gravity="center_vertical"
                                android:id="@+id/linearRowarrowright"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_36pxv"
                                android:layout_width="match_parent"
                                android:orientation="horizontal" style="@style/groupStylewhite_A700">
                                <ImageView
                                    android:id="@+id/imageArrowright"
                                    android:layout_height="@dimen/_18pxh"
                                    android:layout_marginBottom="@dimen/_24pxv"
                                    android:layout_marginStart="@dimen/_29pxh"
                                    android:layout_marginTop="@dimen/_17pxv"
                                    android:layout_width="@dimen/_18pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_arrowright"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtSignOut"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_24pxv"
                                    android:layout_marginEnd="@dimen/_65pxh"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_marginTop="@dimen/_19pxv"
                                    android:layout_weight="0.73"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@{mechanicdashboardVM.mechanicdashboardModel.txtSignOut}"
                                    style="@style/txtInterregular12" tools:text="@string/lbl_sign_out"/>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLineTwoTwo"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_202pxv"
                        android:layout_marginTop="@dimen/_202pxv" android:layout_width="@dimen/_180pxh"/>
                    <View android:background="@color/black_900"
                        android:id="@+id/lineLineThree"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_294pxv"
                        android:layout_marginTop="@dimen/_294pxv" android:layout_width="@dimen/_180pxh"/>
                </FrameLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
