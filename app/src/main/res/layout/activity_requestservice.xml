<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="requestserviceVM" type="com.repairpal.app.modules.requestservice.data.viewmodel.RequestserviceVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearRequestService"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnarrowone"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowarrowone"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <FrameLayout android:id="@+id/frameStackarrowone"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_width="@dimen/_44pxh" style="@style/groupStylewhite_A700">
                        <ImageView android:id="@+id/imageArrowOne"
                            android:layout_gravity="bottom|center"
                            android:layout_height="@dimen/_3pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_5pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_33pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow1"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow1"/>
                    </FrameLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRequestaservi"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginStart="@dimen/_42pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_weight="0.7"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{requestserviceVM.requestserviceModel.txtRequestaservi}"
                        style="@style/txtInterbold12" tools:text="@string/msg_request_a_servi"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end"
                android:id="@+id/txtHowcanweassi"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30pxh"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{requestserviceVM.requestserviceModel.txtHowcanweassi}"
                style="@style/txtInterbold12" tools:text="@string/msg_how_can_we_assi"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowbatteryone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearColumnbatteryone"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.49"
                    android:layout_width="0dp" android:orientation="vertical">
                    <ImageView android:gravity="start"
                        android:id="@+id/imageBatteryOne"
                        android:layout_height="@dimen/_113pxh"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_battery1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_battery1"/>
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtCarBatteryIss"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{requestserviceVM.requestserviceModel.txtCarBatteryIss}"
                        style="@style/txtInterregular12" tools:text="@string/msg_car_battery_iss"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStackflattire"
                    android:layout_height="@dimen/_140pxv"
                    android:layout_marginStart="@dimen/_19pxh" android:layout_width="@dimen/_148pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFlattire"
                        android:layout_gravity="bottom|start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_44pxh"
                        android:layout_marginStart="@dimen/_44pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{requestserviceVM.requestserviceModel.txtFlattire}"
                        style="@style/txtInterregular12" tools:text="@string/lbl_flat_tire"/>
                    <ImageView android:id="@+id/imageFlatOne"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_129pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:scaleType="fitXY"
                        imageRes="@{@drawable/img_flat1}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_flat1"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowfuelone"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageFuelOne"
                    android:layout_height="@dimen/_127pxh"
                    android:layout_width="@dimen/_112pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_fuel1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_fuel1"/>
                <ImageView android:id="@+id/imageLokiOne"
                    android:layout_height="@dimen/_92pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_35pxv"
                    android:layout_width="@dimen/_152pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_loki1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_loki1"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowoutoffuel"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_52pxh"
                android:layout_marginStart="@dimen/_52pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOutofFuel"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.56"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{requestserviceVM.requestserviceModel.txtOutofFuel}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_out_of_fuel"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtKeylock"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_126pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.44"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{requestserviceVM.requestserviceModel.txtKeylock}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_key_lock"/>
            </LinearLayout>
            <FrameLayout android:id="@+id/frameStacktowone"
                android:layout_gravity="end"
                android:layout_height="@dimen/_101pxv"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_46pxv" android:layout_width="@dimen/_332pxh">
                <ImageView android:id="@+id/imageTowOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_98pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_155pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_tow1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_tow1"/>
                <ImageView android:id="@+id/imageOtherOne"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_101pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_width="@dimen/_181pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_other1}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_other1"/>
            </FrameLayout>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowtowing"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTowing"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.35"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{requestserviceVM.requestserviceModel.txtTowing}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_towing"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOtherservice"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_marginStart="@dimen/_114pxh"
                    android:layout_weight="0.65"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{requestserviceVM.requestserviceModel.txtOtherservice}"
                    style="@style/txtInterregular12" tools:text="@string/lbl_other_service"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnConfirmIssue"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_46pxv"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_67pxv"
                android:layout_width="@dimen/_252pxh"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@string/lbl_confirm_issue"
                android:textAllCaps="false" style="@style/btnSolid_1" tools:text="@string/lbl_confirm_issue"/>
        </LinearLayout>
    </LinearLayout>
</layout>
